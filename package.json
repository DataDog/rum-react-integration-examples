{
  "name": "@datadog/rum-react-integration",
  "version": "2.15.1",
  "private": "true",
  "license": "Apache-2.0",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "cjs/index.d.ts",
  "scripts": {
    "build": "run-p build:cjs build:esm build:bundle",
    "build:bundle": "rm -rf bundle && webpack --mode=production",
    "build:cjs": "rm -rf cjs && tsc -p tsconfig.cjs.json",
    "build:esm": "rm -rf esm && tsc -p tsconfig.esm.json",
    "format": "prettier --check .",
    "lint": "scripts/cli lint .",
    "typecheck": "scripts/cli typecheck . && scripts/cli typecheck developer-extension",
    "test": "yarn test:unit:watch",
    "test:unit": "karma start ./test/unit/karma.local.conf.js",
    "test:unit:watch": "yarn test:unit --no-single-run"
  },
  "dependencies": {
    "react-router-dom": "^5.0.0",
    "tslib": "^1.10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DataDog/rum-react-integration.git"
  },
  "peerDependencies": {
    "@datadog/browser-core": "2.15.1",
    "@datadog/browser-rum-core": "2.15.1"
  },
  "devDependencies": {
    "@testing-library/react": "12.0.0",
    "@types/jasmine": "3.6.3",
    "@types/node": "^16.0.0",
    "@types/pako": "1.0.1",
    "@types/react": "16",
    "@types/react-dom": "16",
    "@types/react-router-dom": "^5.1.7",
    "@types/sinon": "9.0.10",
    "@typescript-eslint/eslint-plugin": "4.15.0",
    "@typescript-eslint/parser": "4.15.0",
    "@wdio/jasmine-framework": "6.1.3",
    "@wdio/junit-reporter": "6.1.9",
    "@wdio/local-runner": "6.1.3",
    "@wdio/spec-reporter": "6.0.16",
    "eslint": "7.20.0",
    "eslint-config-prettier": "7.2.0",
    "istanbul-instrumenter-loader": "3.0.1",
    "jasmine-core": "3.6.0",
    "karma": "6.1.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-junit-reporter": "2.0.1",
    "karma-sourcemap-loader": "0.3.8",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "5.0.0",
    "npm-run-all": "^4.1.5",
    "pako": "2.0.3",
    "prettier": "2.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "ts-loader": "8.0.18",
    "tsconfig-paths-webpack-plugin": "3.5.1",
    "typescript": "4.1.5",
    "webpack": "5.28.0"
  }
}
